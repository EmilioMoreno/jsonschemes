{
   "definitions":{
      "list_of_audios":{
         "type":"object",
         "title":"List",
         "$id":"https://raw.githubusercontent.com/EmilioMoreno/jsonschemes/master/list.json",
         "default":null,
         "required":[
            "id",
            "user",
            "username",
            "title",
            "description",
            "imgfile",
            "created",
            "updated",
            "private",
            "collaborative",
            "commentpolicy",
            "followed",
            "numfollowers",
            "numaudios"
         ],
         "properties":{
            "id":{
               "$id":"#/properties/id",
               "type":"string",
               "title":"The Id Schema",
               "default":"",
               "pattern":"^([0-9]+)$"
            },
            "user":{
               "$id":"#/properties/user",
               "type":"string",
               "title":"The User Schema",
               "default":"",
               "pattern":"^([0-9]+)$"
            },
            "username":{
               "$id":"#/properties/username",
               "type":"string",
               "title":"The Username Schema",
               "default":""
              
            },
            "title":{
               "$id":"#/properties/title",
               "type":"string",
               "title":"The Title Schema",
               "default":"",
               "pattern":"^(.*)$"
            },
            "description":{
               "$id":"#/properties/description",
               "type":"string",
               "title":"The Description Schema",
               "default":""
   
            },
            "imgfile":{ "$ref": "https://raw.githubusercontent.com/EmilioMoreno/jsonschemes/master/imagefile.json#/definitions/imagefile_list" },
            "created":{
               "$id":"#/properties/created",
               "type":"integer",
               "title":"The Created Schema",
               "default":0,
               "examples":[
                  1505390369
               ]
            },
            "updated":{
               "$id":"#/properties/updated",
               "type":"integer",
               "title":"The Updated Schema",
               "default":0,
               "examples":[
                  1542376066
               ]
            },
            "private":{
               "$id":"#/properties/private",
               "type":"string",
               "title":"The Private Schema",
               "default":"",
               "examples":[
                  "0"
               ],
               "pattern":"^[4-5]$"
            },
            "collaborative":{
               "$id":"#/properties/collaborative",
               "type":"string",
               "title":"The Collaborative Schema",
               "default":"",
               "examples":[
                  "0"
               ],
               "pattern":"^([0-9]+)$"
            },
            "commentpolicy":{
               "$id":"#/properties/commentpolicy",
               "type":"string",
               "title":"The Commentpolicy Schema",
               "default":"",
               "examples":[
                  "1"
               ],
               "pattern":"^([0-9]+)$"
            },
            "followed":{
               "$id":"#/properties/followed",
               "type":"boolean",
               "title":"The Followed Schema",
               "default":false,
               "examples":[
                  false
               ]
            },
            "numfollowers":{
               "$id":"#/properties/numfollowers",
               "type":"string",
               "title":"The Numfollowers Schema",
               "default":"",
               "examples":[
                  "1"
               ],
               "pattern":"^([0-9]+)$"
            },
            "numaudios":{
               "$id":"#/properties/numaudios",
               "type":"string",
               "title":"The Numaudios Schema",
               "default":"",
               "examples":[
                  "23"
               ],
               "pattern":"^([0-9]+)$"
            }
         }
      }
   }
}

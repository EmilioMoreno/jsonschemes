{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/EmilioMoreno/jsonschemes/master/multisuscription_related.json",
  "type": "array",
  "title": "Multisuscription_list",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "podcast",
      "algorithm"
    ],
    "properties": {
      "podcast": {
        "$id": "#/items/properties/podcast",
        "type": "object",
        "title": "The Podcast Schema",
        "required": [
          "id",
          "name",
          "description",
          "image",
          "imagemd",
          "imagexl",
          "channelname",
          "channelid",
          "ranking",
          "lastupdatetext",
          "lastupdate",
          "lastupdatetimestamp",
          "numaudios",
          "numsuscriptions",
          "paid"
        ],
        "properties": {
          "id": {
            "$id": "#/items/properties/podcast/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "45110"
            ],
            "pattern": "^[0-9]+$"
          },
          "name": {
            "$id": "#/items/properties/podcast/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "M"
            ],
            "pattern": "^(.*)$"
          },
          "description": {
            "$id": "#/items/properties/podcast/properties/description",
            "type": "string",
            "title": "The Description Schema",
            "default": "",
            "examples": [
              "dfdsf"
                  ]
          },
          "img":{ "$ref": "https://raw.githubusercontent.com/EmilioMoreno/jsonschemes/master/imagefile.json#/definitions/imagefile_list" },
          "imagemd": { "$ref": "https://raw.githubusercontent.com/EmilioMoreno/jsonschemes/master/imagefile.json#/definitions/imagefile_list" }, 
          "imagexl": { "$ref": "https://raw.githubusercontent.com/EmilioMoreno/jsonschemes/master/imagefile.json#/definitions/imagefile_list" },
          "channelname": {
            "$id": "#/items/properties/podcast/properties/channelname",
            "type": "string",
            "title": "The Channelname Schema",
            "default": "",
            "examples": [
              "Biografías y Documentales"
            ],
            "pattern": "^(.*)$"
          },
          "channelid": {
            "$id": "#/items/properties/podcast/properties/channelid",
            "type": "string",
            "title": "The Channelid Schema",
            "default": "",
            "examples": [
              "408"
            ],
            "pattern": "^[0-9]+$"
          },
          "ranking": {
            "$id": "#/items/properties/podcast/properties/ranking",
            "type": "string",
            "title": "The Ranking Schema",
            "default": "",
            "examples": [
              "194"
            ],
            "pattern": "^[0-9]+$"
          },
          "lastupdatetext": {
            "$id": "#/items/properties/podcast/properties/lastupdatetext",
            "type": "string",
            "title": "The Lastupdatetext Schema",
            "default": "",
            "examples": [
              "hace +1 año"
            ],
            "pattern": "^(.*)$"
          },
          "lastupdate": {
            "$id": "#/items/properties/podcast/properties/lastupdate",
            "type": "string",
            "title": "The Lastupdate Schema",
            "default": "",
            "examples": [
              "24/03/2015"
            ],
            "pattern": "^[0-3][0-9]/[0-1][0-9]/[1-2][0-9][0-9][0-9]$"
          },
          "lastupdatetimestamp": {
            "$id": "#/items/properties/podcast/properties/lastupdatetimestamp",
            "type": "integer",
            "title": "The Lastupdatetimestamp Schema",
            "default": 0,
            "examples": [
              1427167947
            ]
          },
          "numaudios": {
            "$id": "#/items/properties/podcast/properties/numaudios",
            "type": "string",
            "title": "The Numaudios Schema",
            "default": "",
            "examples": [
              "135"
            ],
            "pattern": "^[0-9]+$"
          },
          "numsuscriptions": {
            "$id": "#/items/properties/podcast/properties/numsuscriptions",
            "type": "string",
            "title": "The Numsuscriptions Schema",
            "default": "",
            "examples": [
              "4146"
            ],
            "pattern": "^[0-9]+"
          },
          "paid": {
            "$id": "#/items/properties/podcast/properties/paid",
            "type": "integer",
            "title": "The Paid Schema",
            "default": 0,
            "examples": [
              0
            ]
          }
        }
      },
      "algorithm": {
        "$id": "#/items/properties/algorithm",
        "type": "string",
        "title": "The Algorithm Schema",
        "default": "",
        "examples": [
          "top_subcat"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}